{
  "include": [
    "api/**/*.ts",
    "bin/**/*.ts",
    "core/**/*.ts",
    "db/**/*.ts",
    "bin/dev.ts",
    "global.d.ts",
    "loader.js",
    "suppress.cjs",
    "tiny/**/*.ts",
    "ui/**/*.ts"
  ],
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowJs": true,
    "alwaysStrict": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "lib": ["dom", "dom.iterable", "es2015", "scripthost"],
    "module": "es2020",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": {
      "@fe/api/*.ts": ["./api/*.ts"],
      "@fe/bin/*.ts": ["./bin/*.ts"],
      "@fe/core/*.ts": ["./core/*.ts"],
      "@fe/db/*.ts": ["./db/*.ts"],
      "@fe/ui/*.ts": ["./ui/*.ts"],
      "@tiny/*.ts": ["./tiny/*.ts"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2020",
    "types": ["node"]
  }
}
