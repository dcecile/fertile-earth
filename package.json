    "prettier": "3.2.5",
    "syncpack": "13.0.0",
    "typescript": "5.4.5"
  },
  "pnpm": {
    "neverBuiltDependencies": [
      "fsevents"
    ]
  },
  "prettier": "@symbolize/dev-prettier/index.json",
  "syncpack": {
    "sortFirst": [
      "name",
      "description",
      "type",
      "version",
      "author",
      "license"
    ],
    "versionGroups": [
      {
        "label": "Please use dev or prod dependencies",
        "packages": [
          "**"
        ],
        "dependencies": [
          "**"
        ],
        "dependencyTypes": [
          "overrides",
          "peer",
          "pnpmOverrides",
          "resolutions"
        ],
        "isBanned": true
      },
      {
        "label": "Use workspace protocol when developing local packages",
        "dependencies": [
          "@symbolize/**"
        ],
        "dependencyTypes": [
          "dev",
          "prod"
        ],
        "pinVersion": "workspace:*"
      }
    ],
    "semverGroups": [
      {
        "range": "",
        "dependencies": [
          "**"
        ],
        "packages": [
          "**"
        ]
      }
    ]
  }
}
